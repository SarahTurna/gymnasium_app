<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="431.0" prefWidth="311.0" style="-fx-background-color: #346154; -fx-border-color: white; -fx-border-width: 3px 3px 3px 3px;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Login.ForgetPasswordController">
   <children>
      <Label layoutX="75.0" layoutY="69.0" text="Reset Pasword" textFill="#d4d4d4">
         <font>
            <Font name="System Bold" size="23.0" />
         </font>
      </Label>
      <TextField fx:id="userIDTextField" layoutX="70.0" layoutY="138.0" prefHeight="27.0" prefWidth="178.0" promptText="User ID" style="-fx-background-color: Transparent; -fx-border-color: #d4d4d4;">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </TextField>
      <TextField fx:id="UserEmailTextField" layoutX="69.0" layoutY="175.0" prefHeight="27.0" prefWidth="178.0" promptText="User Email" style="-fx-background-color: Transparent; -fx-border-color: #d4d4d4;">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </TextField>
      <TextField fx:id="passwordTextField" layoutX="70.0" layoutY="216.0" prefHeight="27.0" prefWidth="178.0" promptText="New Password" style="-fx-background-color: Transparent; -fx-border-color: #d4d4d4;">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </TextField>
      <TextField fx:id="RepasswordTextField" layoutX="70.0" layoutY="256.0" prefHeight="27.0" prefWidth="178.0" promptText="Rewrite Password" style="-fx-background-color: Transparent; -fx-border-color: #d4d4d4;">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </TextField>
      <Button fx:id="confirmButton" layoutX="123.0" layoutY="326.0" mnemonicParsing="false" onAction="#confirmButtonOnClick" text="Confirm" textFill="#000000a7">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
